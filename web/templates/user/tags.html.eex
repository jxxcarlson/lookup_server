<span style="float:right">
  <%= link "Update tags", to: user_path(@conn, :update_tags), class: "btn btn-default" %>
</span>

<h4>Tags: <%= length(@user.tags) %></h4>




<table class="table">
  <tbody>
<%= for tag <- @user.tags do %>
    <tr>
      <td><%= link LookupPhoenix.Tag.pretty(tag), to: search_path(@conn, :tag_search, tag) %> </td>
    </tr>
<% end %>
  </tbody>
</table>
