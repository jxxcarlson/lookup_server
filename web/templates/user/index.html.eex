<h1>Users</h1>


<table class="table">
  <thead>
    <tr>
      <th>id</th>
      <th>Name</th>
      <th>Usename</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
<%= for user <- @users do %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.name %></td>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
    </tr>
<% end %>
  </tbody>
</table>


